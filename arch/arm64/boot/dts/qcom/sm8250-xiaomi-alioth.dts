&tlmm {
	gpio-reserved-ranges = <40 4>, <52 4>;

	mdm2ap_default: mdm2ap-default {
		pins = "gpio1", "gpio3";
		function = "gpio";
		drive-strength = <8>;
		bias-disable;
	};

	ts_active: ts-active {
		pins = "gpio33", "gpio39";
		function = "gpio";
		drive-strength = <8>;
		bias-pull-up;
	};

	ts_int_suspend: ts-int-suspend {
		pins = "gpio39";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-down;
	};

	ts_reset_suspend: ts-reset-suspend {
		pins = "gpio33";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-down;
	};

	pmx_ts_release: pmx-ts-release {
		pins = "gpio33", "gpio39";
		function = "gpio";
		drive-strength = <2>;
		bias-disable;
	};

	ap2mdm_default: ap2mdm-default {
		pins = "gpio56", "gpio57";
		function = "gpio";
		drive-strength = <16>;
		bias-disable;
	};
};

&uart12 {
	status = "okay";
};

